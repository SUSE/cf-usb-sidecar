{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceRoot}/cmd/catalog-service-manager/catalog-service-manager.go",
			"env": {
				"GOPATH": "${workspaceRoot}/Godeps/_workspace:${env.GOPATH}",
				"CSM_HOME": "${workspaceRoot}/examples/mysql",
				"CSM_API_KEY": "csm-auth-token"
			},
			"args": []
		},
		{
			"name": "Launch-unit-test-sample",
			"type": "go",
			"request": "launch",
			"mode": "test",
			// You can only run tests from specific directory here
			// change the value of the program parameter in there and
			// set it to the directory where your test file is
			"program": "${workspaceRoot}/src/connection",
			"env": {
				"GOPATH":"${workspaceRoot}/Godeps/_workspace:${env.GOPATH}",
				"CSM_API_KEY": "csm-auth-token"
				},
			"args": []
		}
	]
}
