{  
   "name":"amazon-rds-mysql",
   "version":"latest",
   "vendor":"amazon",
   "components":[  
      {  
         "name":"csm-side-car",
         "version":"latest",
         "vendor":"amazon",
         "external_name":"amazon-rds-mysql",
         "image":"192.168.99.101:5000/amazon-rds-mysql",
         "min_RAM_mb":512,
         "min_disk_gb":1,
         "min_VCPU":1,
         "platform":"linux-x86_64",
         "workload_type":"container",
         "capabilities":[  

         ],
         "labels":[  
            "database",
            "mysql",
            "aws"
         ],
         "min_instances":1,
         "max_instances":1,
         "service_ports":[  
            {  
               "name":"csm",
               "protocol":"TCP",
               "source_port":8081,
               "target_port":8081,
               "public":true
            }
         ],
         "parameters":[  
            {  
               "name":"AWS_ACCESS_KEY_ID"
            },
            {  
               "name":"AWS_SECRET_ACCESS_KEY"
            },
            {  
               "name":"AWS_RDS_REGION"
            },
            {  
               "name":"MYSQL_RDS_INSTANCE"
            },
            {  
               "name":"MYSQL_DB_CLASS"
            },
            {  
               "name":"DB_SIZE"
            },
            {  
               "name":"MULTIAZ"
            },
            {  
               "name":"MYSQL_ROOT_USER"
            },
            {  
               "name":"MYSQL_ROOT_PASSWORD"
            },
            {  
               "name":"CSM_API_KEY"
            }
         ]
      }
   ],
   "parameters":[  
      {  
         "name":"AWS_ACCESS_KEY_ID",
         "description":"AWS_ACCESS_KEY_ID",
         "data_type":"string",
         "required":true,
         "secret":false
      },
      {  
         "name":"AWS_SECRET_ACCESS_KEY",
         "description":"AWS_SECRET_ACCESS_KEY",
         "data_type":"string",
         "required":true,
         "secret":false
      },
      {  
         "name":"AWS_RDS_REGION",
         "description":"AWS RDS REGION",
         "default":"us-west-2",
         "example":"us-west-2",
         "data_type":"string",
         "required":true,
         "secret":false
      },
      {  
         "name":"MYSQL_RDS_INSTANCE",
         "description":"MYSQL_RDS_INSTANCE",
         "example":"dbinstancename",
         "data_type":"string",
         "required":true,
         "secret":false
      },
      {  
         "name":"MYSQL_DB_CLASS",
         "description":"MYSQL DB CLASS",
         "default":"db.t2.micro",
         "example":"db.t2.micro",
         "data_type":"string",
         "required":true,
         "secret":false
      },
      {  
         "name":"DB_SIZE",
         "description":"DB_SIZE",
         "default":"5",
         "example":"5",
         "data_type":"string",
         "required":true,
         "secret":false
      },
      {  
         "name":"MULTIAZ",
         "description":"MULTIAZ",
         "default":"false",
         "example":"False",
         "data_type":"string",
         "required":true,
         "secret":false
      },
      {  
         "name":"MYSQL_ROOT_USER",
         "description":"MASTER USER FOR THE DATABASE",
         "default":"root_user",
         "example":"root_user",
         "data_type":"string",
         "required":true,
         "secret":false
      },
      {  
         "name":"MYSQL_ROOT_PASSWORD",
         "description":"MYSQL_ROOT_PASSWORD",
         "default":"root_password",
         "example":"root_passwords",
         "data_type":"string",
         "required":true,
         "secret":false
      },
      {  
         "name":"CSM_API_KEY",
         "description":"CSM_API_KEY",
         "default":"csm-auth-token",
         "example":"csm-auth-token",
         "data_type":"string",
         "required":true,
         "secret":false
      }
   ]
}